<div class="movies-title-subtitle">
  <h2>Ironhack Cinema</h2>
  <h3>Click on the movie to check the showtimes!</h3>
</div>

<div class="movie-overview">
  {{#each movies}}
    <div class="movie-card">
      <img src="{{this.image}}" alt="Movie poster" />
      <h4>{{title}}</h4>
      <a href="movies/{{this._id}}">See more</a>
    </div>
  {{/each}}
</div>